<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Photos" table="photos">
    <indexes>
      <index name="IDX_876E0D97E3C61F9" columns="owner_id"/>
      <index name="IDX_876E0D91137ABCF" columns="album_id"/>
    </indexes>
    <unique-constraints>
      <unique-constraint name="UNIQ_876E0D9C5978E527E3C61F9" columns="vk_id,owner_id"/>
    </unique-constraints>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="vkId" type="integer" column="vk_id" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="link" type="string" column="link" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="created" type="integer" column="created" nullable="false">
      <options>
        <option name="unsigned">1</option>
      </options>
    </field>
    <many-to-one field="owner" target-entity="Users" fetch="LAZY">
      <join-columns>
        <join-column name="owner_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="album" target-entity="Albums" fetch="LAZY">
      <join-columns>
        <join-column name="album_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
